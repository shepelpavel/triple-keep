{
	"manifest_version": 2,
	
	"name": "TripleKeep",
	"description": "Simple Triple Notes",
	"version": "1.5",
	
	"icons": {
		"128": "128.png"
	},
	
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	
	"permissions": [
		"activeTab",
		"storage",
		"clipboardRead",
		"clipboardWrite"
	]
}
